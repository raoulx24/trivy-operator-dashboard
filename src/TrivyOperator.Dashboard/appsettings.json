{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "MainAppPort": 8900,
  "Kubernetes": {
    "KubeConfigFileName": "",
    "NamespaceList": "",
    "TrivyUseClusterRbacAssessmentReport": true,
    "TrivyUseConfigAuditReport": true,
    "TrivyUseExposedSecretReport": true,
    "TrivyUseVulnerabilityReport": true,
    "TrivyUseClusterComplianceReport": true,
    "TrivyUseClusterVulnerabilityReport": true,
    "TrivyUseRbacAssessmentReport": true,
    "TrivyUseSbomReport": true,
    "TrivyUseClusterSbomReport": true
  },
  "Queues": {
    "Capacity": 5000
  },
  "Watchers": {
    "WatchTimeoutInSeconds": 600
  },
  "FileExport": {
    "TempFolder": null
  },
  "GitHub": {
    "BaseTrivyDashboardRepoUrl": "https://api.github.com/repos/raoulx24/trivy-operator-dashboard",
    "BaseTrivyOperatorRepoUrl": "https://api.github.com/repos/aquasecurity/trivy-operator",
    "ServerCheckForUpdates": true,
    "CheckForUpdatesIntervalInMinutes": 360
  },
  "OpenTelemetry": {
    "Enabled": true,
    "OtelEndpoint": "",
    "ConsoleEnabled": false,
    "AspNetCoreInstrumentationEnabled": true,
    "RuntimeInstrumentationEnabled": true,
    "OtelProtocol": "grpc",
    "HistogramBoundsInMs": [ 200, 500, 1000, 5000 ],
    "PrometheusExporterPort": 8901
  }
}