    "@types/node": "^22.7.4",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "eslint": "^9.13.0",
    "eslint-plugin-html": "^8.1.2",
    "jasmine-core": "~5.3.0",
    "karma": "~6.4.4",
