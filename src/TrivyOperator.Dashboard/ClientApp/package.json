{
  "name": "trivyoperator.dashboard",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run prebuild && ng serve",
    "build": "ng build",
    "build:sme": "ng build --source-map=true --named-chunks==true && source-map-explorer dist/**/*.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prebuild": "node build/preBuild.js",
    "update-endpoints:local": "curl -L http://localhost:5032/swagger/v1/swagger.yaml -o backend-api.yaml",
    "prettier:cli": "prettier '**/*.{css,scss,md,yml,json,js,ts,xml,html}'",
    "prettier:check": "npm run prettier:cli -- --check",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint:cli": "eslint",
    "eslint:check": "npm run eslint:cli",
    "eslint:fix": "npm run eslint:cli -- --fix",
    "lint": "npm run eslint:check && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.4",
    "@angular/common": "^20.1.4",
    "@angular/compiler": "^20.1.4",
    "@angular/core": "^20.1.4",
    "@angular/forms": "20.1.4",
    "@angular/platform-browser": "^20.1.4",
    "@angular/platform-browser-dynamic": "^20.1.4",
    "@angular/platform-server": "^20.1.4",
    "@angular/router": "^20.1.4",
    "@microsoft/signalr": "^8.0.7",
    "@primeng/themes": "^20.0.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/cytoscape": "^3.21.9",
    "@types/cytoscape-fcose": "^2.2.4",
    "chart.js": "^4.5.0",
    "cytoscape": "3.33.0",
    "cytoscape-fcose": "^2.2.0",
    "ngx-markdown": "^20.0.0",
    "postcss": "^8.5.6",
    "primeicons": "^7.0.0",
    "primeng": "^20.0.1",
    "run-script-os": "^1.1.6",
    "rxjs": "~7.8.2",
    "tailwindcss": "4.1.11",
    "tailwindcss-primeui": "^0.6.1",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.1.4",
    "@angular-eslint/builder": "^20.1.1",
    "@angular-eslint/eslint-plugin": "^20.1.1",
    "@angular-eslint/eslint-plugin-template": "^20.1.1",
    "@angular-eslint/schematics": "^20.1.1",
    "@angular-eslint/template-parser": "^20.1.1",
    "@angular/cli": "^20.1.4",
    "@angular/compiler-cli": "^20.1.4",
    "@types/jasmine": "~5.1.8",
    "@types/jasminewd2": "~2.0.13",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-plugin-html": "^8.1.3",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-openapi-gen": "^0.53.0",
    "prettier": "^3.6.2",
    "prettier-plugin-multiline-arrays": "^4.0.3",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-organize-imports": "^4.2.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.8.3"
  },
  "overrides": {
    "autoprefixer": "10.4.5",
    "webpack": "5.81.0"
  }
}
